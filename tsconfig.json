{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "useDefineForClassFields": true,
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "skipLibCheck": true
  },
  "references": [
    {
      "path": "@app/ydid-client"
    },
    {
      "path": "@app/ydid-instance"
    },
    {
      "path": "packages/ui-lib"
    }
  ]
}
